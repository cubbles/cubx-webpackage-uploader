{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./lib"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "outputSourcePath"      : true,
    "outputSourceFiles": true,
    "systemName": "cubx-webpackage-uploader",
    "footer": "incowia GmbH",
    "copyright": "incowia GmbH Â© 2014-2016",
    "navType": "vertical",
    "theme": "cerulean",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": true

  },
  "opts": {
    "template": "node_modules\\ink-docstrap\\template",
    "encoding": "utf8",
    "destination": "docs/js",
    "recurse": true,
    "private": false,
    "explain" : false
  }
}